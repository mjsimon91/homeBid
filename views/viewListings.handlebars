<!-- Adding a navbar -->
<nav>
  <div class="container">
    <div class="row">
      <div class="nav-wrapper">
        <a href="./home.html" class="brand-logo">homeBid</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
          <li><a href="./listings.html">Buy a Home</a></li>
          <li><a href="./createListing.html">Sell a Home</a></li>
          <li><a href="badges.html">Login</a></li>
        </ul>
      </div>
    </div>
  </div>
</nav>

<div class="container">
  <div class="row">

    {{#each listings}}
      <div class="col s12 m4">

        <div class="card">
          <div class="card-image">
            <a href="#"><img src="./css/Images/home.jpeg"></a>
            <span class="card-title">{{this.dataValues.title}}</span>
            <span class="card-content"><i class="tiny material-icons">visibility</i> {{this.dataValues.viewCount}}</span>
            <span class="card-content"><i class="tiny material-icons">gavel</i> {{this.dataValues.bidCount}}</span>
          </div>
          <div class="card-content">
            <p>{{this.dataValues.description}}</p>
          </div>
          <div class="card-action">
            <p class="card-title">${{this.dataValues.bidAmount}}</p>
          </div>
        </div>

      </div>
    {{/each}}

  </div>
</div>
