{{#each bids}}
<div class="row">
  <div class="col s12 m12">
    <div class="card horizontal hoverable">

      <div class="card-image">
        <img src="/css/Images/home.jpeg">
      </div>

      <div class="card-stacked">
        <div class="card-content">

          <!-- Replace this with dynamic address data -->
          <p>{{this.dataValues.Home.addressOne}}</p>
          <p>{{this.dataValues.Home.addressTwo}}</p>
          <p>{{this.dataValues.Home.city}}, {{this.dataValues.Home.state}} {{this.dataValues.Home.zipCode}}</p>

        </div>

        <div class="card-action">
          <!-- Replace this with the number of bids that have been made -->
          <p>My Latest Bid: ${{this.dataValues.bidAmount}}</p>
          <p class="won">Won</p>
        </div>
      </div>

    </div>
  </div>
</div>
{{/each}}
